<launch>
    <!-- Arguments -->
    <arg name="slam_methods" default="cartographer" doc="slam type [gmapping, cartographer, hector, karto]"/>

    <!-- Rviz -->
    <node pkg="rviz" type="rviz" name="rviz" required="true"
         args="-d $(find windplanner)/rviz/real_$(arg slam_methods).rviz"/>
</launch>
